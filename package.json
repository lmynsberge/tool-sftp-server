{
  "name": "test-node-app",
  "version": "1.0.0",
  "description": "To test local things",
  "main": "index.js",
  "scripts": {
    "compile": "tsc --outDir bld --module commonjs --target ES2020",
    "prod": "catalyze src/server.ts",
    "test": "mocha --exit --require ts-node/register 'test/**/*.spec.ts'",
    "test:debug": "npm run test -- --debug-brk",
    "start": "ts-node-dev --respawn ./src/server.ts",
    "watch": "nodemon --inspect 'small-server.js' -e js,ts",
    "run-scratch": "ts-node-dev --respawn ./scratch.ts",
    "prettify": "prettier --write \"src/**/*.{js,jsx,json,yml,yaml,html,css,scss,ts,tsx,less}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,json,yml,yaml,html,css,scss,ts,tsx,less}\"",
    "lint": "eslint --ext .ts,.tsx src --color --max-warnings=0",
    "lint:clean": "eslint --fix --ext .ts,.tsx src --color"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.2.2",
    "@types/lodash": "4.14.165",
    "@types/mocha": "5.2.7",
    "@types/sinon": "7.0.13",
    "@types/sinon-chai": "3.2.3",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "chai": "4.2.0",
    "eslint": "7.13.0",
    "jest": "26.6.3",
    "mocha": "8.2.1",
    "prettier": "2.1.2",
    "sinon": "7.4.2",
    "sinon-chai": "3.3.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/ssh2-sftp-client": "2.5.1",
    "axios": "0.19.2",
    "cors": "2.8.5",
    "express": "^4.17.1",
    "lodash": "4.17.20",
    "mllp-node": "2.0.0",
    "ssh2-sftp-client": "4.0.4",
    "uuid": "8.3.1"
  }
}
